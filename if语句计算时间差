#include<stdio.h>
int main(void)
{
	int hour1, minutes1;
	int hour2, minutes2;
	printf("请输入想要计算时间差的两个时间：\n");
	scanf_s("%d %d", &hour1, &minutes1);
	scanf_s("%d %d", &hour2, &minutes2);
	int ih = hour1 - hour2;
	int im = minutes1 - minutes2;
	if (im < 0) {
		im = 60 + im;
		ih--;
	}
	printf("时间差是%d小时%d分。\n", ih, im);
	return 0;
}

